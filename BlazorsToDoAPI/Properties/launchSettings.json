{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56599",
      "sslPort": 44385
    }
  },
  "profiles": {
    "BlazorsToDoAPI-DEV": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7258;http://localhost:5258",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGODB_USER": "your-mongodb-user",
        "MONGODB_PASS": "your-mongodb-password",
        "MONGODB_SERVER": "your-mongodb-server",
        "MONGODB_BASENAME": "your-mongodb-database-name",
        "TASK_COLLECTION": "your-mongodb-taskcollection-name",
        "USERS_COLLECTION": "your-mongodb-usercollection-name",
        "JWT_SECRET": "your-jwt-secret"
      }
    },
